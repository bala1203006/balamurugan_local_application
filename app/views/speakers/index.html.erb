<%= link_to 'move to event page', events_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% @speakers.each do |speaker| %>

  <tbody>
      <tr>
        <td><%= speaker.name %></td>
        <td><%= link_to 'Show', speaker_path(speaker) %></td>
         <td><%= link_to 'Edit', edit_speaker_path(speaker) %></td>
          <td><%= link_to 'Destroy', speaker_path(speaker), method: :delete, data: { confirm: 'Are you sure?' } %></td>
             </tr>
  </tbody>
<% end %>
</table>
