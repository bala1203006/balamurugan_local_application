<%= link_to 'move to event page', events_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% @sponsors.each do |sponsor| %>

  <tbody>
      <tr>
        <td><%= sponsor.name %></td>
        <td><%= link_to 'Show', sponsor_path(sponsor) %></td>
         <td><%= link_to 'Edit', edit_sponsor_path(sponsor) %></td>
          <td><%= link_to 'Destroy', sponsor_path(sponsor), method: :delete, data: { confirm: 'Are you sure?' } %></td>
             </tr>
  </tbody>
<% end %>
</table>
<%= link_to 'click here to assign sponsor to event', new_sponsor_path %>
